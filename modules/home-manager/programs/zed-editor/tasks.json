[
	{
		"label": "File Finder",
		"command": "zed \"$(tv files)\"",
		"hide": "always",
		"allow_concurrent_runs": true,
		"use_new_terminal": true
	},
	{
		"label": "Find in Files",
		"command": "zed \"$(tv text)\"",
		"hide": "always",
		"allow_concurrent_runs": true,
		"use_new_terminal": true
	},
	{
		"label": "Find in File",
		"command": "bat --style=numbers --color=always --theme=TwoDark --paging=never $env.ZED_FILE | tv --ansi --input-header=$\"($env.ZED_RELATIVE_FILE)\" | ansi strip | str trim | split row \" \" | first | zeditor $\"($env.ZED_FILE):($in)\"",
		"hide": "always",
		"allow_concurrent_runs": true,
		"use_new_terminal": true,
		"shell": { "program": "nu" }
	},
	{
		"label": "Lazyjj",
		"command": "lazyjj",
		"hide": "on_success",
		"reveal_target": "center",
		"show_summary": false,
		"show_command": false,
		"allow_concurrent_runs": true,
		"use_new_terminal": true
	}
]
